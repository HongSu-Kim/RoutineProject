<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
		PUBLIC "-/mybatis.org//DTD mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.soo.routine.mapper.BoardMapper">

	<select id="findAllByCategoryAndMemberNum" resultType="com.soo.routine.entity.Board" parameterType="map">
		select * from board where category = #{category} and memberNum = #{memberNum}
	</select>

</mapper>
