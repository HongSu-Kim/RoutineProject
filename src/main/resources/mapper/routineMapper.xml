<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
		PUBLIC "-/mybatis.org//DTD mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.soo.routine.mapper.RoutineMapper">

	<select id="findAllByMemberNum" resultType="com.soo.routine.entity.Routine" parameterType="int">
		select * from routine where memberNum = #{memberNum}
	</select>

</mapper>
