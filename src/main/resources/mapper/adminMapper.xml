<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
		PUBLIC "-/mybatis.org//DTD mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.soo.routine.mapper.AdminMapper">

	<select id="getBoard" resultType="com.soo.routine.domain.Board" parameterType="int">
		SELECT * FROM BOARD WHERE BOARDNUM = #{boardNum}
	</select>
	
	<select id="getBoardList" resultType="com.soo.routine.domain.Board" parameterType="String">
		SELECT * FROM BOARD WHERE CATEGORYNAME = #{categoryName}
	</select>



</mapper>
